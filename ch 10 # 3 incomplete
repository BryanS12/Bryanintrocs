import random

# My Name: Bryan Su

def average(numList):
    a= 0 
    for i in range(len(numList)):
        a += i
    l = a/len(numList)
    
    return l      

def makeRandRange(numVals, start, end, seed=None):
    random.seed(seed) 
    new = []
    for i in range(numVals):
        a = random.randrange(start, end+1)
        new.append(a)
    print(new)
    return average(new)
   


print(makeRandRange( 20 , 1, 5))
